# URLs da aplicação a ser testada
URL_ALVO=https://sua-aplicacao.com/app/home
BASE_URL=https://sua-aplicacao.com/
EXPECTED_PATH=/app/home

# Credenciais de login
LOGIN_USERNAME=seu.usuario
LOGIN_PASSWORD=suasenha123
LOGIN_ID_VERIFICACAO=40
LOGIN_ID_SISTEMA=11
LOGIN_ID_CONTRATO=7
LOGIN_NOME_SISTEMA=SEU_SISTEMA
LOGIN_CONTRATO_NOME=NOME DO CONTRATO

# Seletores CSS para login (ajuste conforme sua aplicação)
LOGIN_SELECTOR_USERNAME=#email
LOGIN_SELECTOR_PASSWORD=#password
LOGIN_SELECTOR_SUBMIT=button:contains('Entrar')

# Paleta de cores permitidas (hexadecimal, separadas por vírgula)
PALETA_CORES_PERMITIDAS=#ffffff,#000000,#f8f9fa,#1e293b,#1f3f6e

# Screenshots
ENABLE_SCREENSHOTS=true

# Tokens adicionais para busca além de "null" e "NaN"
# Separe por vírgula ou nova linha
SEARCH_TEXTS=erro,indefinido,falha,exception

# Configurações do Puppeteer
HEADLESS=false
SLOWMO=50
MAXIMIZE=true
VIEWPORT_WIDTH=1920
VIEWPORT_HEIGHT=1080

# Configurações de navegação (opcional - para sistemas complexos)
# Tipos disponíveis: generic, angular_fuse, bootstrap, react
NAV_TYPE=generic

